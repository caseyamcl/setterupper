{
    "name": "caseyamcl/setterupper",
    "type": "library",
    "description": "A framework-independent package to assist in console-based setup scripts",
    "keywords": ["caseyamcl", "setterupper", "Setup", "Framework-agnostic"],
    "homepage": "https://github.com/caseyamcl/setterupper",
    "license": "MIT",
    "authors": [
        {
            "name": "Casey McLaughlin",
            "email": "caseyamcl@gmail.com",
            "homepage": "https://caseymclaughlin.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.4",
        "psr/log": "^1.1",
        "caseyamcl/sortable-tasks": "^0.5|^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/console": "^3.4|^4.4|^5.0"
    },
    "autoload": {
        "psr-4": {
            "SetterUpper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SetterUpper\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
